<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Authors>Artur Kraev</Authors>
    <Description>Advanced .Net API for RabbitMQ</Description>
    <Copyright>Artur Kraev 2015-2017</Copyright>
    <PackageProjectUrl>https://github.com/rabbit-link/rabbit-link</PackageProjectUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/rabbit-link/rabbit-link/master/LICENSE</PackageLicenseUrl>
    <PackageTags>RabbitMQ Messaging AMQP</PackageTags>
    <VersionPrefix>0.5.1</VersionPrefix>
    <VersionSuffix>beta</VersionSuffix>
    <PackageReleaseNotes>Global rewrite library
</PackageReleaseNotes>
  </PropertyGroup>
  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFrameworks>netstandard1.6;net46</TargetFrameworks>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="RabbitMQ.Client" Version="5.0.1" />
  </ItemGroup>
</Project>